{
  "model_name": "baseline_lstm_energy_demand",
  "version": "1.0.0",
  "target_variable": "energy_demand",
  "feature_names": [
    "energy_demand",
    "solar_generation",
    "wind_generation",
    "temperature",
    "hour",
    "day_of_week",
    "is_weekend",
    "renewable_penetration"
  ],
  "sequence_length": 48,
  "forecast_horizons": [
    1,
    6,
    24
  ],
  "lstm_units": [
    64,
    32
  ],
  "dropout_rate": 0.2,
  "learning_rate": 0.001,
  "preprocessing": {
    "feature_scaling": "MinMaxScaler",
    "target_scaling": "StandardScaler"
  }
}